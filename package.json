{
  "name": "auto-script",
  "description": "A simple script to reduce duplicate work.",
  "version": "0.0.0",
  "author": "fiv5",
  "bin": {
    "auto-script": "./bin/run",
    "as": "./bin/run"
  },
  "bugs": "https://github.com/work/auto-script/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.3.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "^6.0.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/work/auto-script",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "auto-script",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "work/auto-script",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
